---
title: Postoversikt
layout: layouts/page.njk
---
<style>
.list-item {
  list-style-type: none;
}
</style>

<section class="postlist mt-3 pt-3">
  <div class="container">
    <ul class="list">
      {% for post in collections.post %}
      <li class="list-item">
        {{page.date|readableDate}}: <a href="{{ post.url }}">{{ post.data.tittel }}</a>
      </li>
      {% endfor %}
    </ul>
    </div>
  </div>
</section>