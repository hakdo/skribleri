---
title: Reiserelatert
layout: layouts/page.njk
---
<style>.list-item {list-style-type: none;}</style>

<section class="postlist mt-3 pt-3">
  <div class="container">
    <ul class="list">
      {% for post in collections.reise %}
      <li class="list-item mb-3">
        {{post.data.date|readableDate}}: <a href="{{ post.url }}">{{ post.data.tittel }}</a><br>
        {{post.data.beskrivelse}}
      </li>
      {% endfor %}
    </ul>
    </div>
  </div>
</section>